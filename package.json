        "@testing-library/react-native": "^9.1.0",
        "@types/jest": "^27.4.1",
        "@types/react-native": "^0.67.3",
        "@types/react-test-renderer": "^17.0.1",
        "babel-jest": "^26.6.3",
        "detox": "^19.6.5",
        "eslint": "7.14.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.66.2",
        "react-test-renderer": "17.0.2"
    },
    "jest": {
        "preset": "react-native",
        "transformIgnorePatterns": [
